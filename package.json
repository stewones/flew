{
  "name": "@ionfire/reactive-record",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test reactive-record --code-coverage --source-map --watch=false --progress=false --browsers=ChromeHeadlessCI",
    "test:w": "ng test reactive-record --code-coverage --source-map --progress=false --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:w": "ng build reactive-record --prod --watch",
    "build:p": "ng build reactive-record --prod",
    "patch:custom": "standard-version release --release-as 1.0.1 && bin/patch-warning.sh",
    "patch:build": "standard-version release --release-as build && bin/patch-warning.sh",
    "patch:patch": "standard-version release --release-as patch && bin/patch-warning.sh",
    "patch:minor": "standard-version release --release-as minor && bin/patch-warning.sh",
    "patch:major": "standard-version release --release-as major && bin/patch-warning.sh",
    "patch:a": "standard-version release --release-as patch --prerelease alpha && bin/patch-warning.sh",
    "patch:b": "standard-version release --release-as patch --prerelease beta && bin/patch-warning.sh",
    "publish": "npm run build:p && cp README.md dist/reactive-record && cd dist/reactive-record && npm publish && git push origin --tags"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~7.0.0-beta.5",
    "@angular/compiler": "~7.0.0-beta.5",
    "@angular/core": "~7.0.0-beta.5",
    "@angular/platform-browser": "~7.0.0-beta.5",
    "@angular/platform-browser-dynamic": "~7.0.0-beta.5",
    "axios": "^0.18.0",
    "core-js": "^2.5.4",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.9.0-beta.1",
    "@angular-devkit/build-ng-packagr": "~0.9.0-beta.1",
    "@angular/cli": "~7.0.0-beta.4",
    "@angular/compiler-cli": "~7.0.0-beta.5",
    "@angular/language-service": "~7.0.0-beta.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.0.1"
  },
  "prettier": {
    "singleQuote": true,
    "jsxBracketSameLine": true
  }
}
